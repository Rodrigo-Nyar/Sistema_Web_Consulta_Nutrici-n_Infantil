<h1>revisionMedica</h1>


<% if (result.length == 0) { %>
  <h4>
    0 revisionMedica
  </h4>
<% } else { %>
<table>
	<thead>
    <th>#</th>
		<th>id</th>
			<th>createdAt</th>
			<th>updatedAt</th>
			<th>id</th>
			<th>lugar</th>
			<th>peso</th>
			<th>estatura</th>
			<th>cabeza</th>
			<th>fecha</th>
			<th>idPaciente</th>

		<th>created at</th>
		<th>updated at</th>
		<th colspan="3"></th>
	</thead>

	<tbody>
		<% result.forEach((item, i) => { %>
			<tr>
				<td>
					<%= (i + 1) %>
				</td>
				<td>
					<%= item.id %>
				</td>
					<td>
						<%= item.createdAt %>
					</td>
					<td>
						<%= item.updatedAt %>
					</td>
					<td>
						<%= item.id %>
					</td>
					<td>
						<%= item.lugar %>
					</td>
					<td>
						<%= item.peso %>
					</td>
					<td>
						<%= item.estatura %>
					</td>
					<td>
						<%= item.cabeza %>
					</td>
					<td>
						<%= item.fecha %>
					</td>
					<td>
						<%= item.idPaciente %>
					</td>
				<td>
					<%= item.createdAt %>
				</td>
				<td>
					<%= item.updatedAt %>
				</td>
				<td>
					<a href="/revisionMedica/show/<%= item.id %>">Show</a>
				</td>
				<td>
					<a href="/revisionMedica/delete/<%= item.id %>">Delete</a>
				</td>
        <td>
					<a href="/revisionMedica/edit/<%= item.id %>">Edit</a>
				</td>
			</tr>
			<% }) %>
	</tbody>
</table>

<% } %>

<br>
<a href="/revisionMedica/create">New</a>