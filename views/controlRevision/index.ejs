<h1>controlRevision</h1>


<% if (result.length == 0) { %>
  <h4>
    0 controlRevision
  </h4>
<% } else { %>
<table>
	<thead>
    <th>#</th>
		<th>id</th>
			<th>createdAt</th>
			<th>updatedAt</th>
			<th>id</th>
			<th>idMedico</th>
			<th>idConsultorio</th>
			<th>idRevisionMedica</th>

		<th>created at</th>
		<th>updated at</th>
		<th colspan="3"></th>
	</thead>

	<tbody>
		<% result.forEach((item, i) => { %>
			<tr>
				<td>
					<%= (i + 1) %>
				</td>
				<td>
					<%= item.id %>
				</td>
					<td>
						<%= item.createdAt %>
					</td>
					<td>
						<%= item.updatedAt %>
					</td>
					<td>
						<%= item.id %>
					</td>
					<td>
						<%= item.idMedico %>
					</td>
					<td>
						<%= item.idConsultorio %>
					</td>
					<td>
						<%= item.idRevisionMedica %>
					</td>
				<td>
					<%= item.createdAt %>
				</td>
				<td>
					<%= item.updatedAt %>
				</td>
				<td>
					<a href="/controlRevision/show/<%= item.id %>">Show</a>
				</td>
				<td>
					<a href="/controlRevision/delete/<%= item.id %>">Delete</a>
				</td>
        <td>
					<a href="/controlRevision/edit/<%= item.id %>">Edit</a>
				</td>
			</tr>
			<% }) %>
	</tbody>
</table>

<% } %>

<br>
<a href="/controlRevision/create">New</a>