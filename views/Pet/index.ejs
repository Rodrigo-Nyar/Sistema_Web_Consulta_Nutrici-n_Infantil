<h1>Pet</h1>


<% if (result.length == 0) { %>
  <h4>
    0 Pet
  </h4>
<% } else { %>
<table>
	<thead>
    <th>#</th>
		<th>id</th>
			<th>name</th>
			<th>owner</th>

		<th>created at</th>
		<th>updated at</th>
		<th colspan="3"></th>
	</thead>

	<tbody>
		<% result.forEach((item, i) => { %>
			<tr>
				<td>
					<%= (i + 1) %>
				</td>
				<td>
					<%= item.id %>
				</td>
					<td>
						<%= item.name %>
					</td>
					<td>
						<%= item.owner %>
					</td>
				<td>
					<%= item.createdAt %>
				</td>
				<td>
					<%= item.updatedAt %>
				</td>
				<td>
					<a href="/Pet/show/<%= item.id %>">Show</a>
				</td>
				<td>
					<a href="/Pet/delete/<%= item.id %>">Delete</a>
				</td>
        <td>
					<a href="/Pet/edit/<%= item.id %>">Edit</a>
				</td>
			</tr>
			<% }) %>
	</tbody>
</table>

<% } %>

<br>
<a href="/Pet/create">New</a>